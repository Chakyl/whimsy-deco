{
  "multipart": [
    {
      "when": { "AND": [  { "facing": "north" } ] },
      "apply": { "model": "whimsy_deco:block/gray_cash_register" }
    },
    {
      "when": { "AND": [ { "open": true }, { "facing": "north" } ] },
      "apply": { "model": "whimsy_deco:block/gray_cash_register_open" }
    },
    {
      "when": { "AND": [  { "facing": "south" } ] },
      "apply": { "model": "whimsy_deco:block/gray_cash_register", "y": 180 }
    },
    {
      "when": { "AND": [ { "open": true }, { "facing": "south" } ] },
      "apply": { "model": "whimsy_deco:block/gray_cash_register_open", "y": 180 }
    },
    {
      "when": { "AND": [  { "facing": "east" } ] },
      "apply": { "model": "whimsy_deco:block/gray_cash_register", "y": 90 }
    },
    {
      "when": { "AND": [ { "open": true }, { "facing": "east" } ] },
      "apply": { "model": "whimsy_deco:block/gray_cash_register_open", "y": 90 }
    },
    {
      "when": { "AND": [ {  "facing": "west" } ] },
      "apply": { "model": "whimsy_deco:block/gray_cash_register", "y": 270 }
    },
    {
      "when": { "AND": [ { "open": true }, { "facing": "west" } ] },
      "apply": { "model": "whimsy_deco:block/gray_cash_register_open", "y": 270 }
    }
  ]
}